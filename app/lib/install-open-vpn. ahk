FileCopy, C:\config.ovpn, C:\carpeta-donde-este-instalacion-open-vpn\config.ovpn
FileCopy, C:\config.key, C:\carpeta-donde-este-instalacion-open-vpn\config.key
FileCopy, C:\otro_config.key, C:\carpeta-donde-este-instalacion-open-vpn\otro_config.key
